    "@supabase/supabase-js": "^2.49.1",
    "@types/express": "^5.0.0",
    "angularx-qrcode": "^21.0.4",
    "cally": "^0.9.2",
    "chart.js": "^4.4.8",
    "express": "^4.21.2",
    "jspdf": "^4.2.0",
    "jspdf-autotable": "^5.0.7",
    "ng2-charts": "^8.0.0",
    "ngx-mask": "^21.0.1",
    "ngx-skeleton-loader": "^12.0.0",
