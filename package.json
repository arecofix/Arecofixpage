    "gh-pages": "^6.3.0",
    "trim-newlines": "^3.0.1",
    "uglify-js": "^3.19.3",
    "ajv": "^8.17.2",
    "minimatch": "^10.2.2"
  },
  "allowedCommonJsDependencies": [
    "qrcode"
  ],
  "private": true,
  "dependencies": {
    "@angular/cdk": "^21.1.5",
    "@angular/common": "^21.1.5",
    "@angular/compiler": "^21.1.5",
    "@angular/core": "^21.1.5",
    "@angular/forms": "^21.1.5",
    "@angular/platform-browser": "^21.1.5",
    "@angular/platform-server": "^21.1.5",
    "@angular/router": "^21.1.5",
    "@angular/ssr": "^21.1.5",
    "@capacitor/android": "^8.1.0",
