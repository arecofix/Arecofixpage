{
  "name": "arecofix",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "rm -rf .angular && ng serve",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:dev": "ng build --configuration development",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy": "npm run build && npm run generate-sitemap && node scripts/remove-preloads.mjs",
    "generate-sitemap": "node scripts/generate-sitemap.mjs"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
